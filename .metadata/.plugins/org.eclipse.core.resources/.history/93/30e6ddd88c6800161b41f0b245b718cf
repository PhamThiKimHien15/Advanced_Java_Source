package Ex1_13;

import phamThiKimHien.Employee;

public class BusinessEmployee extends Employee {
	private double salary;
	private float sales;
	public BusinessEmployee(String name, double ceoSalary, int numOfPeople,
			double subsidize, double salary, int sales) {
		super(name, ceoSalary, numOfPeople, subsidize);
		this.salary = salary;
		this.sales = sales;
	}
	public double getSalary() {
		return salary;
	}
	public void setSalary(double salary) {
		this.salary = salary;
	}
	public float getSales() {
		return sales;
	}
	public void setSales(int sales) {
		this.sales = sales;
	}
	public double calcBonuses() {
		return salary*sales;
	}
}
