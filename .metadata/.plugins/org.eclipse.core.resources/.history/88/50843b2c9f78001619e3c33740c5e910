package Ex4_1;

import java.util.Arrays;
import java.util.List;

public class MainStream {
	public static void main(String[] args) {
		List<String> strings = Arrays.asList("happy", "sad", "happy", " ",
				"oil", " ", "sad", "oil", "happy", "interesting", "sad", "interesting", "abc");
		// count the element have value " "
		int count = (int) strings.stream().filter(string -> string.contains(" ")).count();
		System.out.println("Element null = " + count);
		
		// count the element have length >= 5
		int countLenght = (int) strings.stream().filter(n -> strings.size() >= 5).count();
		System.out.println("Element have length >= 5 = " + countLenght);
	}
}
