package Ex5_4;

public class Counter extends Thread {
	private int count;
	private int lo;
	private int hi;

	public Counter(int count, int lo, int hi) {
		super();
		this.count = count;
		this.lo = lo;
		this.hi = hi;
	}

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}
	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			this.count = count ++;
		}
	}
}
