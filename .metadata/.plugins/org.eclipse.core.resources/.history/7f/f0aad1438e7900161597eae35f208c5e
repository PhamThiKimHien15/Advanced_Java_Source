package Ex5_4;

public class Counter extends Thread {
	private int count;

	public Counter(int count) {
		super();
		this.count = count;
	}
	
	@Override
	public void run() {
		for (int i = this.count; i <= (this.count*10); i++) {
			this.count = i;
			Thread.sleep(1000);
		}
	}
	
	public int getCount() {
		return count;
	}
	
	public void setCount(int count) {
		this.count = count;
	}
	
}
