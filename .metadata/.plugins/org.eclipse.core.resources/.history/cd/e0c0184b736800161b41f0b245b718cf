package Ex1_12;

import java.util.ArrayList;

public class ListTextBook {
	ArrayList<TextBook> list;
	
	public ListTextBook(){
		this.list = new ArrayList<TextBook>();
	}
	
	public void addTextBook(TextBook tex) {
		list.add(tex);
	}
	
	public void showInfo() {
		for (int i = 0; i < list.size(); i++) {
			
		}
	}
}
