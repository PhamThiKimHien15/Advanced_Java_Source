package Ex5_4;

public class Counter extends Thread {
	private int count;
	private int[] arr;
	private int lo;
	private int hi;

	public Counter(int[] arr, int lo, int hi) {
		super();
		this.arr = arr;
		this.lo = lo;
		this.hi = hi;
	}

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}
	@Override
	public void run() {
		for (int i = this.count; i <= (this.count*10); i++) {
			this.count = count ++;
		}
	}
}
