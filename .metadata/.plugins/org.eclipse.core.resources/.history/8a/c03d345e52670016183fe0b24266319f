package unitTest;

import static org.junit.Assert.*;

import org.junit.Test;

import phamThiKimHien.Distance;
import phamThiKimHien.Point;
/*
 * Author: Pham Thi Kim Hien
 * Date: 08/21/2016
 * Version: 1.0
 * 
 * Unit Test for calcDistance() method of Distance() class
 */
public class TestDistance {

	@Test
	public void test() {
		phamThiKimHien.Distance dis = new Distance();
		Point pointA = new Point(0, 0);
		Point pointB = new Point(5, 6);
		assertFalse(dis.calcDistance(pointA,pointB) == 3);
	}

}
