package Ex1_16;

import java.util.ArrayList;

public class ListCar {

	ArrayList<Car> list;
	
	public ListCar(){
		this.list = new ArrayList<Car>();
	}
	public void add(Car car) {
		list.add(car);
	}
	public void showInfo() {
		for (int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i).toString());
		}
	}
}
